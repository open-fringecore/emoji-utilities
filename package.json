{
  "name": "emoji-utilities",
  "version": "1.0.1",
  "description": "An ever expanding library of emoji utilities. (tree-shakable)",
  "type": "module",
  "exports": {
    "./handshake-builder": "./dist/handshake-builder.mjs"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch --preserveWatchOutput",
    "dev": "pnpm run build:watch",
    "test": "jest",
    "prepublishOnly": "pnpm test && pnpm run build"
  },
  "keywords": [
    "emoji", "utilities", "handshake"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "5.4.3",
    "jest": "29.7.0",
    "@types/jest": "29.5.12",
    "ts-jest": "29.1.2"
  }
}
